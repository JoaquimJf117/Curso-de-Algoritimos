algoritmo "ConteAte10"
var
   valor, salto, contador: Inteiro
inicio
   contador <- 0
   Escreva("Quer Contar Até quanto? ")
   Leia(Valor)
   Escreva("Qual sera o valor do Salto? ")
   Leia(salto)
   Enquanto (contador <= valor) faca
      EscrevaL(Contador)
      contador <- contador + salto
   FimEnquanto
   Escreva(" Terminei Minha Contagem!")
fimalgoritmo
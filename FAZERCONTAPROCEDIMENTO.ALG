algoritmo "FazerContaProcedimento"
var
   X, Y:Inteiro
Procedimento Soma(A, B: Inteiro)
inicio
      EscrevaL(" Recebi o Valor: ", A)
      EscrevaL(" Recebi o valor: ", B)
      EscrevaL(" A soma entre os dois Valores e ", A + B)
FimProcedimento
inicio
      X <- 5
      Y <- 3
      Soma(X, Y)
fimalgoritmo